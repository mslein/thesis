	langid = {english},
	file = {Holt et al. - 2003 - Impacts of environmental variability in open popul.pdf:/Users/maslein/Zotero/storage/WQ6CRQUH/Holt et al. - 2003 - Impacts of environmental variability in open popul.pdf:application/pdf}
}

@article{kroeker_ecological_2020,
