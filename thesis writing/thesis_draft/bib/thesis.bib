	file = {Spanoudis et al. - 2015 - Impact of fluctuating temperatures on development .pdf:/Users/maslein/Zotero/storage/Z6FJ9U5F/Spanoudis et al. - 2015 - Impact of fluctuating temperatures on development .pdf:application/pdf}
}

