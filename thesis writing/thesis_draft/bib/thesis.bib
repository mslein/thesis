@article{manenti_predictability_2014-2,
	title = {Predictability rather than amplitude of temperature fluctuations determines stress resistance in a natural population of \textit{Drosophila simulans}},
	volume = {27},
	issn = {1010061X},
	url = {http://doi.wiley.com/10.1111/jeb.12463},
	doi = {10.1111/jeb.12463},
	abstract = {The adaptability of organisms to novel environmental conditions depends on the amount of genetic variance present in the population as well as on the ability of individuals to adjust their phenotype through phenotypic plasticity. Here, we investigated the phenotypic plasticity induced by a single generation’s exposure to three different temperature regimes with respect to several life-history and stress-resistance traits in a natural population of Drosophila simulans. We studied a constant as well as a predictably and an unpredictably ﬂuctuating temperature regime. We found high levels of phenotypic plasticity among all temperature regimes, suggesting a strong inﬂuence of both temperature ﬂuctuations and their predictability. Increased heat tolerance was observed for ﬂies developed in both types of ﬂuctuating thermal environments compared with ﬂies developed in a constant environment. We suggest that this was due to beneﬁcial hardening when developing in either ﬂuctuating temperature environment. To our surprise, ﬂies that developed in constant and predictably changing environments were similar to each other in most traits when compared to ﬂies from the unpredictably ﬂuctuating environment. The unpredictably changing thermal environment imposed the most stressful condition, resulting in the lowest performance for stress-related traits, even though the absolute temperature changes never exceeded that of the predictably ﬂuctuating environment. The overall decreased stress resistance of ﬂies in the unpredictably ﬂuctuating environment may be the consequence of maladaptive phenotypic plasticity in this setting, indicating that the adaptive value of plasticity depends on the predictability of the environment.},
	pages = {2113--2122},
	number = {10},
	journaltitle = {J. Evol. Biol.},
	author = {Manenti, T. and Sørensen, J. G. and Moghadam, N. N. and Loeschcke, V.},
	urldate = {2021-03-09},
	date = {2014-10},
	langid = {english},
	file = {Manenti et al. - 2014 - Predictability rather than amplitude of temperatur.pdf:/Users/maslein/Zotero/storage/NSYFGR7K/Manenti et al. - 2014 - Predictability rather than amplitude of temperatur.pdf:application/pdf}
}

